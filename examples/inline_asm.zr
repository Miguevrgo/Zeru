fn syscall_1(nr: i64, a1: i32) i64 {
    var result: i64 = 0;
    asm volatile (
        "syscall"
        : "={rax}" (result)
        : "{rax}" (nr), "{rdi}" (a1)
        : "rcx", "r11", "memory"
    );
    return result;
}

fn main() {
    var sys_exit: i64 = 60;
    var status: i32 = 0;
    syscall_1(sys_exit, status);
    exit(status);
}

fn main() {
    var x: i32 = 0;

    // Loop test
    while x < 50 {
        x = x + 1;
    }

    // Branch test
    if x == 50 {
        x = 100;
    } else {
        x = 0;
    }

    while x < 150 {
        x += 1;
        if x == 120 {
            continue;
        } else if x == 123 {
            break;
        }
    }
}


fn double(x: i32) i32 {
    return x * 2;
}

fn increment(var x: i32) i32 {
    x += 1;
    return x;
}

fn process(var counter: i32, limit: i32) i32 {
    while counter < limit {
        counter += 1;
    }
    return counter;
}

fn main() i32 {
    var a = increment(5);
    var b = double(10);
    var c = process(0, 10);
    return a + b + c;
}
